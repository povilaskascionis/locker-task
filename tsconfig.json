    "noImplicitAny": true,
    "outDir": "./dist",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["./src/index.tsx"]
}
