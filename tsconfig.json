{
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./dist",
    "preserveConstEnums": true,
    "esModuleInterop": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "global.d.ts"]
}
